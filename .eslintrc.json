{
  "extends": [
    "eslint-config-kyt"
  ],

  "parser": "babel-eslint",

  "env": {
  },

  "plugins": [
  ],

  "rules": {
    "jsx-a11y/no-static-element-interactions": 0,
    "arrow-parens": 0,
    "react/no-unused-prop-types": [
      2,
      {
        "skipShapeProps": true
      }
    ]
  }
}
